#!/usr/bin/env bash

BUILD_DIR=$1

# Exit early if app is not compatible
if [ ! -d $BUILD_DIR/backend/requirements.txt ]; then
  echo "Could not find requirements.txt"
  exit 1
fi

if [ ! -d $BUILD_DIR/frontend/package.json ]; then
  echo "Could not find package.json"
    exit 1
fi

echo py-plus-node
